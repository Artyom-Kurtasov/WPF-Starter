<Window x:Class="WPF_Starter.View.ExportToExcel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Starter.View"
        mc:Ignorable="d"
        Title="ExportToExcel" Height="450" Width="800">
    <Grid>
        <TextBox 
            HorizontalAlignment="Left" 
            Margin="211,199,0,0" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            Width="312" 
            UndoLimit="400"
            x:Name="SearchTextBox"/>
        <TextBlock HorizontalAlignment="Left" Margin="320,154,0,0" TextWrapping="Wrap" Text="Введите" VerticalAlignment="Top"/>
        <Button Content="Button" 
                HorizontalAlignment="Left" 
                Margin="554,318,0,0"
                VerticalAlignment="Top"
                Width="77" 
                RenderTransformOrigin="0.5,0.5"
                Command="{Binding ExportToExcelFile}">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
    </Grid>
</Window>
